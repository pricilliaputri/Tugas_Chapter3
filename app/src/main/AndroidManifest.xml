<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tugas_Chapter3"
        tools:targetApi="31">
        <activity
            android:name=".parcelable.ParcelableActivity2"
            android:exported="false" />
        <activity
            android:name=".parcelable.ParcelableActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".serializable.SerializableActivity2"
            android:exported="true" />
        <activity
            android:name=".serializable.SerializableActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".bundle.BundleActivity2"
            android:exported="false" />
        <activity
            android:name=".bundle.BundleActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>